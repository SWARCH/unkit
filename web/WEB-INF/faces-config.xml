<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

   
    
    <managed-bean>
        <managed-bean-name>customerBean</managed-bean-name>
        <managed-bean-class>presentation.bean.CustomerBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>      
    </managed-bean>
   
    <navigation-case>
            <from-outcome>editEmployee</from-outcome>
            <to-view-id>/editEmployee.xhtml</to-view-id> 
    </navigation-case>
    <navigation-case>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/usuario.xhtml</to-view-id> 
    </navigation-case>
    <navigation-case>
            <from-outcome>formulario</from-outcome>
            <to-view-id>/formulario.xhtml</to-view-id> 
    </navigation-case>
    
    
   
     <managed-bean>
        <managed-bean-name>userBean</managed-bean-name>
        <managed-bean-class>presentation.bean.UserBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>      
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>employeeBean</managed-bean-name>
        <managed-bean-class>presentation.bean.EmployeeBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    
    <managed-bean>
        <managed-bean-name>vehicleBean</managed-bean-name>
        <managed-bean-class>presentation.bean.VehicleBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>VehicleBean</managed-bean-name>
        <managed-bean-class>presentation.bean.VehicleBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>partBean</managed-bean-name>
        <managed-bean-class>presentation.bean.PartBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>cart</managed-bean-name>
        <managed-bean-class>presentation.bean.Cart</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    
  
      
    <navigation-rule>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <!--<redirect/>-->>
        </navigation-case>
        <navigation-case>
            <from-outcome>employee</from-outcome>
            <to-view-id>/employeeRol/employee.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>indexEmployee</from-outcome>
            <to-view-id>/employeeRol/indexEmployee.xhtml</to-view-id> 
    </navigation-case>
        <navigation-case>
            <from-outcome>editEmployee</from-outcome>
            <to-view-id>/employeeRol/editEmployee.xhtml</to-view-id>
        </navigation-case>   
        <navigation-case>
            <from-outcome>mayorista</from-outcome>
            <to-view-id>/customerMayorista.xhtml</to-view-id>
            
        </navigation-case>  
        <navigation-case>
            <from-outcome>ensambladora</from-outcome>
            <to-view-id>/customerEnsambladora.xhtml</to-view-id>
            
        </navigation-case>  
<navigation-case>
            <from-outcome>cliente</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            
        </navigation-case>  
        <navigation-case>
            <from-outcome>manager</from-outcome>
            <to-view-id>/management/manager.xhtml</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>editPassword</from-outcome>
            <to-view-id>/management/editPassword.xhtml</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>managementEmployee</from-outcome>
            <to-view-id>/management/managementEmployee.xhtml</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>managementParts</from-outcome>
            <to-view-id>/management/managementParts.xhtml</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>managementVehicles</from-outcome>
            <to-view-id>/management/managementVechicles.xhtml</to-view-id>            
        </navigation-case>
        <navigation-case>
            <from-outcome>Can't find the user</from-outcome>
            <to-view-id>/registration.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
       <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
